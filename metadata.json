{
  "name": "JohnsonEarls-gpgfile",
  "version": "0.1.2",
  "author": "Johnson Earls",
  "summary": "Install a gpg-encrypted file onto a system, decrypting it locally on the system.",
  "license": "Apache-2.0",
  "source": "https://github.com/jearls/puppet-gpgfile",
  "project_page": "https://github.com/jearls/puppet-gpgfile",
  "issues_url": "https://github.com/jearls/puppet-gpgfile/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 1.0.0 < 5.0.0"
    }
  ],
  "data_provider": "none",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5.0",
        "6.0"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=2.7.0 <5.0.0"
    }
  ],
  "tags": [
    "file",
    "gpg",
    "encryption",
    "decryption"
  ]
}
